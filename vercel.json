{
    "version" : 2,
    "builds" : [
        {
            "src" : "src/server.ts",
            "use" : "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.ts",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "https://flynte.vercel.app",
                "Access-Control-Allow-Credentials" : true
            }
        }
    ]
}